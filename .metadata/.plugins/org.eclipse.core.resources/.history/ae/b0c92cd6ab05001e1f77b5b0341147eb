package entity;

public class User {
	private String userName;
	private String userNumber;
	private int userAge;
	private String userSex;
	private int daysRemaining;
	private int managerNo;
	private String[] consultationInfo;
	
	
	public void setName(String userName) {
		this.userName = userName;
	}
	public void setNum(String userNumber) {
		this.userNumber = userNumber;
	}
	public void setAge(int userAge) {
		this.userAge = userAge;
	}
	public void setSex(String userSex) {
		this.userSex = userSex;
	}
	public void setDays(int daysRemaining) {
		this.daysRemaining = daysRemaining;
	}
	public void setManagerNo(int managerNo) {
		this.managerNo = managerNo;
	}
	public void addConsultation(String info) {
		// 미구현 부분
	}
	public void delConsultation(String info) {
		// 미구현 부분
	}
	public String getName() {
		return this.userName;
	}
	public String getNum() {
		return this.userNumber;
	}
	public int getAge() {
		return this.userAge;
	}
	public String getSex() {
		return this.userSex;
	}
	public int getDays() {
		return this.daysRemaining;
	}
	public int getManagerNo() {
		return this.managerNo;
	}
	
	public User(String userName, String userSex, int userAge, String userNumber, int daysRemaining, int managerNo) {
		setName(userName);
		setNum(userNumber);
		setAge(userAge);
		setSex(userSex);
		setDays(daysRemaining);
		setManagerNo(managerNo);
	}
	
}
